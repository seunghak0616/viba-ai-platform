# Node.js 백엔드 환경 변수
NODE_ENV=development
PORT=5000

# AI 마이크로서비스 URL
AI_SERVICE_URL=http://localhost:8000

# 데이터베이스
DATABASE_URL=file:./dev.db

# JWT 시크릿
JWT_SECRET=your-jwt-secret-key-here

# Redis (선택사항)
REDIS_URL=redis://localhost:6379

# CORS 설정
CORS_ORIGIN=http://localhost:3000

# 로그 레벨
LOG_LEVEL=info

# 업로드 설정
UPLOAD_MAX_SIZE=50mb
UPLOAD_DIR=./uploads